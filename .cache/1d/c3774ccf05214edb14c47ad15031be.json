reateElement('div');\n        moduleElement.textContent = module.name;\n        moduleElement.classList.add('module-item');\n        moduleElement.setAttribute('data-module-name', module.name);\n        moduleElement.addEventListener('drop', drop);\n        moduleElement.addEventListener('dragover', allowDrop);\n        secondaryModulesContainer.appendChild(moduleElement);\n    });\n}\n// Función cuando comienza el arrastre\nfunction dragStart(event) {\n    event.dataTransfer.setData(\"text/plain\", event.target.id);\n}\nfunction dragEnd(event) {\n    event.dataTransfer.setData(\"text/plain\", event.target.id);\n}\n\n\n// Función para cargar los profesores de secundaria\nfunction cargarProfesoresSecundarios() {\n    const secondaryProfessorsContainer = document.querySelector('.secondary-professors');\n    secondaryProfessors.forEach(professor => {\n        const professorElement = document.createElement('div');\n        professorElement.textContent = professor;\n        professorElement.classList.add('professor', 'secondary');\n        professorElement.draggable = true;\n        professorElement.addEventListener('dragstart', dragStart);\n        professorElement.addEventListener('dragend', dragEnd);\n        secondaryProfessorsContainer.appendChild(professorElement);\n    });\n}\n\n// Definir la función cuando el elemento arrastrable se suelta en el módulo de secundaria\nfunction drop(event) {\n    const moduleName = event.target.textContent;\n    const professorName = event.dataTransfer.getData('text');\n    \n    // Encontrar el módulo correspondiente\n    const module = secondaryModules.find(module => module.name === moduleName);\n\n    if (module && !module.professors.includes(professorName)) {\n        module.professors.push(professorName);\n        event.target.textContent += `\\n${professorName}`;\n    }\n}\n\n// Cargar los módulos de secundaria y los profesores de secundaria\ncargarModulosSecundarios();\ncargarProfesoresSecundarios();\n"},"lineCount":null}},