ce":"index.js","name":"moduloId","original":{"line":102,"column":44},"generated":{"line":1,"column":2032}},{"source":"index.js","original":{"line":102,"column":55},"generated":{"line":1,"column":2034}},{"source":"index.js","name":"profesorId","original":{"line":102,"column":73},"generated":{"line":1,"column":2050}},{"source":"index.js","original":{"line":102,"column":86},"generated":{"line":1,"column":2052}},{"source":"index.js","name":"horas","original":{"line":102,"column":96},"generated":{"line":1,"column":2060}},{"source":"index.js","original":{"line":102,"column":104},"generated":{"line":1,"column":2062}},{"source":"index.js","name":"draggableElement","original":{"line":104,"column":12},"generated":{"line":1,"column":2072}},{"source":"index.js","name":"classList","original":{"line":104,"column":29},"generated":{"line":1,"column":2074}},{"source":"index.js","name":"add","original":{"line":104,"column":39},"generated":{"line":1,"column":2084}},{"source":"index.js","original":{"line":104,"column":43},"generated":{"line":1,"column":2088}}],"sources":{"index.js":"// Importa la biblioteca interactjs para habilitar la funcionalidad de arrastrar y soltar\r\nimport interact from 'interactjs';\r\n\r\n// Espera a que el DOM se cargue completamente antes de ejecutar el código\r\ndocument.addEventListener('DOMContentLoaded', function () {\r\n    // Habilita la funcionalidad de arrastrar para todos los elementos con la clase CSS 'modulo'\r\n    interact('.modulo').draggable({\r\n        inertia: true, // Habilita la inercia al arrastrar\r\n        modifiers: [ // Define modificadores para el arrastre\r\n            interact.modifiers.restrictRect({ // Restringe el arrastre dentro del contenedor padre\r\n                restriction: 'parent', // Restringe al padre del elemento arrastrable\r\n                endOnly: true // Solo aplica la restricción al final del arrastre\r\n            })\r\n        ],\r\n        autoScroll: true, // Habilita el desplazamiento automático durante el arrastre\r\n        listeners: { // Define listeners de eventos para el arrastre\r\n            start (event) { // Se ejecuta al iniciar el arrastre\r\n                console.log('Arrastrando módulo:', event.target.id);\r\n            },\r\n            move (event) { // Se ejecuta durante el arrastre\r\n                const target = event.target;\r\n                const x = (parseFloat(target.getAttribute('data-x')) || 0) + event.dx; // Calcula la nueva posición X\r\n                const y = (parseFloat(target.getAttribute('data-y')) || 0) + event.dy; // Calcula la nueva posición Y\r\n\r\n                target.style.transform = `translate(${x}px, ${y}px)`; // Aplica la transformación CSS para mover el elemento\r\n                target.setAttribute('data-x', x); // Actualiza la posición X en los datos del elemento\r\n                target.setAttribute('data-y', y); // Actualiza la posición Y en los datos del elemento\r\n            },\r\n            end (event) { // Se ejecuta al finalizar el arrastre\r\n                console.log('Módulo soltado:', event.target.id);\r\n            }\r\n        }\r\n    });\r\n\r\n    // Habilita las zonas de soltado para todos los elementos con la clase CSS 'dropzone'\r\n    interact('.dropzone').dropzone({\r\n        accept: '.modulo', // Solo acepta elementos con la clase CSS 'modulo'\r\n        overlap: 0.75, // Requiere un 75% de superposición para soltar\r\n\r\n        // Escucha los eventos relacionados con el soltar\r\n        ondropactivate: function (event) { // Se ejecuta al activar la zona de soltado\r\n            event.target.classList.add('drop-active'); // Agrega una clase de estilo para indicar que la zona está activa\r\n        },\r\n        ondragenter: function (event) { // Se ejecuta al arrastrar sobre la zona de soltado\r\n            var draggableElement = event.relatedTarget; // Elemento arrastrado\r\n            var dropzoneElement = event.target; // Zona de soltado\r\n\r\n            dropzoneElement.classList.add('drop-target'); // Agrega una clase de estilo para indicar que el elemento puede soltarse aquí\r\n            draggableElement.classList.add('can-drop'); // Agrega una clase de estilo para indicar que el elemento puede soltarse aquí\r\n        },\r\n        ondragleave: function (event) { // Se ejecuta al salir del área de soltado\r\n            event.target.classList.remove('drop-target'); // Remueve la clase de estilo de la zona de soltado\r\n            event.relatedTarget.classList.remove('can-drop'); // Remueve la clase de estilo del elemento arrastrado\r\n        },\r\n        ondrop: function (event) { // Se ejecuta al soltar un elemento sobre la zona de soltado\r\n            var draggableElement = event.relatedTarget; // Elemento arrastrado\r\n            var dropzoneElement = event.target; // Zona de soltado\r\n            var profesorId = dropzoneElement.parentElement.getAttribute('id'); // Obtiene el ID del profesor\r\n            var moduloId = draggableElement.getAttribute('data-modulo'); // Obtiene el ID del módulo\r\n            var horas = parseInt(draggableElement.getAttribute('data-horas')); // Obtiene la cantidad de horas del módulo\r\n\r\n            console.log(\"Asignar Módulo \" + moduloId + \" al Profesor \" + profesorId + \" con \" + horas + \" horas\");\r\n\r\n            draggableElement.classList.add('dropped'); // Agrega una clase al elemento para indicar que ha sido soltado\r\n        },\r\n        ondropdeactivate: function (event) { // Se ejecuta al desactivar la zona de soltado\r\n            event.target.classList.remove('drop-active'); // Remueve la clase de estilo que indica que la zona está activa\r\n            event.target.classList.remove('drop-target'); // Remueve la clase de estilo que indica que el elemento puede soltarse aquí\r\n        }\r\n    });\r\n\r\n    // Habilita las zonas de soltado para las zonas de los profesores\r\n    interact('.profesor .dropzone').dropzone({\r\n        accept: '.modulo', // Solo acepta elementos con la clase CSS 'modulo'\r\n        overlap: 0.75, // Requiere un 75% de superposición para soltar\r\n\r\n        // Escucha los eventos relacionados con el soltar\r\n        ondropactivate: function (event) { // Se ejecuta al activar la zona de soltado\r\n            event.target.classList.add('drop-active'); // Agrega una clase de estilo para indicar que la zona está activa\r\n        },\r\n        ondropdeactivate: function (event) { // Se ejecuta al desactivar la zona de soltado\r\n            event.target.classList.remove('drop-active'); // Remueve la clase de estilo que indica que la zona está activa\r\n        },\r\n        ondragenter: function (event) { // Se ejecuta al arrastrar sobre la zona de soltado\r\n            var draggableElement = event.relatedTarget; // Elemento arrastrado\r\n            var dropzoneElement = event.target; // Zona de soltado\r\n\r\n            dropzoneElement.classList.add('drop-target'); // Agrega una clase de estilo para indicar que el elemento puede soltarse aquí\r\n            draggableElement.classList.add('can-drop'); // Agrega una clase de estilo para indicar que el elemento puede soltarse aquí\r\n        },\r\n        ondragleave: function (event) { // Se ejecuta al salir del área de soltado\r\n            event.target.classList.remove('drop-target'); // Remueve la clase de estilo de la zona de soltado\r\n            event.relatedTarget.classList.remove('can-drop'); // Remueve la clase de estilo del elemento arrastrado\r\n        },\r\n        ondrop: function (event) { // Se ejecuta al soltar un elemento sobre la zona de soltado\r\n            var draggableElement = event.relatedTarget; // Elemento arrastrado\r\n            var dropzoneElement = event.target; // Zona de soltado\r\n            var profesorId = dropzoneElement.parentElement.getAttribute('id'); // Obtiene el ID del profesor\r\n            var moduloId = draggableElement.getAttribute('data-modulo'); // Obtiene el ID del módulo\r\n            var horas = parseInt(draggableElement.getAttribute('data-horas')); // Obtiene la cantidad de horas del módulo\r\n\r\n            console.log(\"Asignar Módulo \" + moduloId + \" al Profesor \" + profesorId + \" con \" + horas + \" horas\");\r\n\r\n            draggableElement.classList.add('dropped'); // Agrega una clase al elemento para indicar que ha sido soltado\r\n        }\r\n    });\r\n});\r\n"},"lineCount":null}},"error":null,"hash":"dcfa54085a4ceed49d5b760268734bde","cacheData":{"env":{}}}