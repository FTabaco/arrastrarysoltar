{\r\n    const technicalModulesContainer = document.getElementById('technical-modules');\r\n    technicalModules.forEach(module => {\r\n        const moduleElement = document.createElement('div');\r\n        moduleElement.textContent = module.name;\r\n        moduleElement.classList.add('module-item');\r\n        moduleElement.setAttribute('data-module-name', module.name);\r\n        moduleElement.addEventListener('drop', drop);\r\n        moduleElement.addEventListener('dragover', allowDrop);\r\n        technicalModulesContainer.appendChild(moduleElement);\r\n    });\r\n}\r\n\r\n// Función para cargar los profesores técnicos\r\nfunction cargarProfesoresTecnicos() {\r\n    const technicalProfessorsContainer = document.querySelector('.technician-professors');\r\n    technicalProfessors.forEach(professor => {\r\n        const professorElement = document.createElement('div');\r\n        professorElement.textContent = professor;\r\n        professorElement.classList.add('professor', 'technician');\r\n        professorElement.draggable = true;\r\n        professorElement.addEventListener('dragstart', dragStart);\r\n        professorElement.addEventListener('dragend', dragEnd);\r\n        technicalProfessorsContainer.appendChild(professorElement);\r\n    });\r\n}\r\n\r\n// Función para permitir el soltar\r\nfunction allowDrop(event) {\r\n    event.preventDefault();\r\n}\r\n\r\n// Función para soltar el elemento\r\nfunction drop(event) {\r\n    const moduleName = event.target.textContent;\r\n    const professorName = event.dataTransfer.getData('text');\r\n    \r\n    // Encontrar el módulo correspondiente\r\n    const module = technicalModules.find(module => module.name === moduleName);\r\n\r\n    if (module && !module.professors.includes(professorName)) {\r\n        module.professors.push(professorName);\r\n        event.target.textContent += `\\n${professorName}`;\r\n    }\r\n}\r\n\r\n// Definir la función cuando inicia el arrastre\r\nfunction dragStart(event) {\r\n    event.dataTransfer.setData('text', event.target.textContent.trim());\r\n}\r\n\r\n// Definir la función cuando termina el arrastre\r\nfunction dragEnd() {\r\n    // No es necesario hacer nada en este caso\r\n}\r\n\r\ncargarModulosTecnicos();\r\ncargarProfesoresTecnicos();\r\n"},"lineCount":null}},"error":null,"hash":"422e585f1013b0e6a149d778fe2ad6ac","cacheData":{"env":{}}}